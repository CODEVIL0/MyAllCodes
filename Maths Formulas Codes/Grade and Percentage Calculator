import time
time.sleep(5)



sub1 = int(input("Enter Marks out of 100 of Hindi/maths: "))

sub2 = int(input("Enter Marks out of 100 of bst/history: "))

sub3 = int(input("Enter Marks out of 100 of accounts/politics: "))

sub4 = int(input("Enter Marks out of 100 of english: "))

sub5 = int(input("Enter Marks out of 100 of economics: "))
print("\n")



total = sub1+sub2+sub3+sub4+sub5

print("Your Total Marks: ",total)
print("\n")


result = ((total/500)*100)



print("Your Percentage is %0.2f"%(result))

print("\n")



if result>75:

    print("Your Grade is A")

elif result >=65 and result <=75:

    print("Your Grade is B")

elif result >45 and result <65:

    print("Your Grade is C")

elif result >=35 and result <45:

    print("Your Grade is D")

else:

    print("Your Grade is E")

